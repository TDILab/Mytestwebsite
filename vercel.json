{
  "headers": [
    {
      "source": "/(.*\\.html)$",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, must-revalidate, max-age=0" }
      ]
    },
    {
      "source": "/(.*\\.(js|css|png|jpg|jpeg|webp|svg|gif|woff2|woff))$",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
